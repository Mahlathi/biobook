<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.biobook"
          android:versionCode="1"
          android:versionName="1.0">
    <uses-sdk android:minSdkVersion="11"/>
    <application android:label="@string/app_name" android:theme="@android:style/Theme.Holo">

        <activity android:name=".views.main"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".views.reg">
            <intent-filter>
                <action android:name="com.example.biobook.views.REG"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".views.upload">
            <intent-filter>
                <action android:name="com.example.biobook.views.UPLOAD"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".views.dispo">
        <intent-filter>
            <action android:name="com.example.biobook.views.DISPO"/>
            <category android:name="android.intent.category.DEFAULT"/>
        </intent-filter>
        </activity>
    </application>
</manifest>
